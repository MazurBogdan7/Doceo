<Page x:Class="Doceo.View.MainUserOffice"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Doceo.View" xmlns:viewmodel="clr-namespace:Doceo.ViewModel" d:DataContext="{d:DesignInstance Type=viewmodel:PagesVM}"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MainUserOffice">

    <Grid Background="#7fc7ff">
        <StackPanel Margin="39,39,393,332" Background="#1faee9">
            <TextBlock x:Name="login" FontSize="20"></TextBlock>
            <PasswordBox x:Name="pasw" PasswordChar="*" IsEnabled="False"></PasswordBox>
        </StackPanel>
    </Grid>
</Page>
